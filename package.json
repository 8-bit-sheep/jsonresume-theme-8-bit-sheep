{
  "name": "jsonresume-theme-8-bit-sheep",
  "version": "1.2.2",
  "private": false,
  "description": "8-bit-sheep JSON Resume Theme",
  "main": "dist/index.js",
  "scripts": {
    "typecheck": "tsc --noEmit",
    "build": "npm run clean && esbuild src/index.tsx --minify --bundle --outfile=dist/index.js --target=node16 --platform=node && cp index.html dist/index.html",
    "clean": "rm -rf dist",
    "create-example-html": "resume export build/example.html --format html --theme ./dist",
    "create-example-pdf": "resume export build/example.pdf --format pdf --theme ./dist",
    "dev": "vite",
    "lint": "eslint .",
    "test": "vitest",
    "prepack": "npm run typecheck && npm run build && npm run lint && CI=true npm run test && npm run create-example-html && npm run create-example-pdf"
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/8-bit-sheep/jsonresume-theme-8-bit-sheep.git"
  },
  "author": "8-bit-sheep",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/8-bit-sheep/jsonresume-theme-8-bit-sheep/issues"
  },
  "homepage": "https://github.com/8-bit-sheep/jsonresume-theme-8-bit-sheep#readme",
  "dependencies": {
    "cheerio": "^0.22.0",
    "date-fns": "^2.29.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-markdown": "^6.0.3",
    "styled-components": "^5.3.9",
    "styled-normalize": "^8.0.7",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@types/cheerio": "^0.22.31",
    "@types/react": "^18.0.29",
    "@types/react-dom": "^18.0.11",
    "@types/styled-components": "^5.1.26",
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "@vitejs/plugin-react": "^3.1.0",
    "@vitest/coverage-c8": "^0.29.8",
    "esbuild": "^0.17.16",
    "eslint": "^8.37.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "jsdom": "^21.1.1",
    "resume-cli": "^3.0.8",
    "typescript": "^5.0.2",
    "vite": "^4.2.1",
    "vitest": "^0.29.8"
  }
}
